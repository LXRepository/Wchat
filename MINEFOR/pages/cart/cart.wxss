/* pages/cart/cart.wxss */
page{
  font-size: 14px;
  width: 100%;
  height: 100%;
}
.addplacemsg{
  margin: 0 auto;
  width: 80%;
  height: 110rpx;
  line-height: 95rpx;
  text-align: center;
  box-sizing: border-box;
  border: 3px dashed #bebebe;
}
.lists{
  margin-top: 30rpx;
  width: 100%;
  box-shadow: 0px -4px 2px #DEDEDE;
}
.lists-li{
  width: 100%;
  height: 300rpx;
  border-bottom: 1px solid #DEDEDE;
  box-sizing: border-box;
  padding: 10rpx;
  overflow: hidden;
  position: relative
}
.lists image{
  width: 206rpx;
  height: 214rpx;
  float: left;
  margin-right: 30rpx;
}
.title{
  float: left;
  width: 60%;
  margin-top: 40rpx;
}
.count{
  float: left;
  margin-top: 87rpx;
  color: #bebebe;
}
.spend{
  float: left;
  position: absolute;
  right: 20rpx;
  bottom: 30rpx;
}
.none{
  width: 100%;
  height: 100%;
}
.none image{
  width: 100%;
  height: 100%;
}