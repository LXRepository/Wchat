/* pages/addmydress/addmydress.wxss */
page{
  width: 100%;
  height: 100%;
  background-color: #f5f5f5;
}
view{
  width: 100%;
}
label{
  width: 100%;
  float: left;
  padding: 20rpx 30rpx;
  background-color: white;
  box-shadow: 0px 2px 2px #DEDEDE;
  margin-bottom: 20rpx;
}
image{
  width: 50rpx;
  height: 50rpx;
  float: left
}
.search{
  width: 80%;
  height: 50rpx;
  background-color: #f5f5f5;
  font-size: 12px;
  border-radius: 20rpx;
  padding: 0 20rpx;
}




.content{
  float: left;
  width: 100%;
}
.content view{
  width: 100%;
  height: 155rpx;
  padding: 20rpx;
  box-sizing: border-box;
  margin-bottom: 4rpx;
  background-color: white;
}
.content view .t-1{
  font-size: 14px;
  display: block;
}
.content view .t-2{
  font-size: 12px;
  display: block;
  color: #b3a9a9;
}